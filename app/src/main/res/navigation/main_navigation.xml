<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation.xml"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.velox.org.features.login.LoginFragment"
        android:label="LoginFragment"
        tools:layout="@layout/fragment_login">
        <action
            android:id="@+id/action_to_dialerFragment"
            app:destination="@id/dialerFragment" />
    </fragment>
    <fragment
        android:id="@+id/dialerFragment"
        android:name="com.velox.org.features.dialer.DialerFragment"
        android:label="DialerFragment"
        tools:layout="@layout/fragment_dialer"
        />

</navigation>
